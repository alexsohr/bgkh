<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns='http://www.ehcache.org/v3'
         xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache-template name="simple">
        <expiry>
            <ttl unit="seconds">3600</ttl>
        </expiry>
        <heap>100</heap>
    </cache-template>

    <cache alias="com.bgkh.domain.User" uses-template="simple"/>

    <cache alias="com.bgkh.domain.Authority" uses-template="simple"/>

    <cache alias="com.bgkh.domain.User.authorities" uses-template="simple"/>

    <cache alias="com.bgkh.domain.PersistentToken" uses-template="simple"/>

    <cache alias="com.bgkh.domain.User.persistentTokens" uses-template="simple"/>

    <cache alias="com.bgkh.domain.Asset" uses-template="simple"/>

    <cache alias="com.bgkh.domain.Asset.workOrders" uses-template="simple"/>

    <cache alias="com.bgkh.domain.WorkOrder" uses-template="simple"/>

    <cache alias="com.bgkh.domain.AssetSpecificationTypeField" uses-template="simple"/>

    <cache alias="com.bgkh.domain.AssetSpecificationTypeField.fields" uses-template="simple"/>

    <cache alias="com.bgkh.domain.AssetSpecificationTypeValue" uses-template="simple"/>

    <cache alias="com.bgkh.domain.UploadFile" uses-template="simple"/>

    <cache alias="com.bgkh.domain.UploadFile.assets" uses-template="simple"/>

    <cache alias="com.bgkh.domain.UploadFile.assetMaps" uses-template="simple"/>

    <cache alias="com.bgkh.domain.UploadFile.assetOtherFiles" uses-template="simple"/>

    <cache alias="com.bgkh.domain.Asset.maps" uses-template="simple"/>

    <cache alias="com.bgkh.domain.Asset.assetOtherFiles" uses-template="simple"/>

    <cache alias="com.bgkh.domain.Asset.otherFiles" uses-template="simple"/>

    <cache alias="com.bgkh.domain.Asset.assetSpecificationTypes" uses-template="simple"/>

    <cache alias="com.bgkh.domain.AssetSpecificationType" uses-template="simple"/>

    <cache alias="com.bgkh.domain.WorkOrderTemplate" uses-template="simple"/>

    <cache alias="com.bgkh.domain.WorkOrder.assets" uses-template="simple"/>

    <cache alias="com.bgkh.domain.WorkOrder.workOrderTemplates" uses-template="simple"/>

    <!-- jhipster-needle-ehcache-add-entry -->
</config>
